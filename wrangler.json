{
  "$schema": "https://cdn.jsdelivr.net/npm/wrangler@latest/config-schema.json",
  "name": "url-shortener-app",
  "compatibility_date": "2025-03-17",
  "no_bundle": true,
  "send_metrics": false,
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "will-be-replaced-during-build"
    }
  ],
  "assets": {
    "binding": "ASSETS"
  },
  "vars": {
    "AUTH_TOKEN": "secret"
  }
}
